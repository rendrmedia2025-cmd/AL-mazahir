{
  "name": "enterprise-dynamic-platform",
  "version": "2.0.0",
  "private": true,
  "description": "Enterprise-grade industrial trading platform for Al Mazahir Trading Est.",
  "keywords": [
    "enterprise",
    "b2b",
    "industrial",
    "trading",
    "next.js",
    "supabase"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=\"\\.test\\.(ts|tsx)$\"",
    "test:property": "jest --testPathPatterns=\"\\.property\\.test\\.(ts|tsx)$\"",
    "test:integration": "jest --testPathPatterns=\"\\.integration\\.test\\.(ts|tsx)$\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:smoke": "node scripts/health-check.js",
    "test:health": "node scripts/health-check.js",
    "format": "prettier --write .",
    "deploy:local": "node scripts/deploy-local.js",
    "start:local": "node scripts/start-server.js",
    "server": "node server.js",
    "check": "node scripts/check-deployment.js",
    "setup:supabase": "node scripts/setup-supabase.js",
    "validate:supabase": "node scripts/validate-supabase.js",
    "check:feature-flags": "node scripts/check-feature-flags.js",
    "deployment:update-status": "node scripts/update-deployment-status.js",
    "deployment:rollback": "node scripts/rollback-deployment.js",
    "analyze": "ANALYZE=true npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npm run security:audit && npm run lint",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "monitoring:check": "node scripts/check-alerts.js",
    "performance:test": "lighthouse-ci autorun"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@sentry/nextjs": "^8.55.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "critters": "^0.0.23",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "next": "16.1.1",
    "node-cron": "^3.0.3",
    "rate-limiter-flexible": "^5.0.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-security": "^3.0.1",
    "fast-check": "^4.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "supabase": "^1.200.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
